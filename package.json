    "eslint": "^8.12.0",
    "express": "^4.17.3",
    "mocha": "^9.2.2",
    "mochawesome": "^7.1.3",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "showdown": "^2.0.3"
  }
}
